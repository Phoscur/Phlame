{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/react" },
  "defaultProject": "app",
  "generators": {
    "@nrwl/react": {
      "application": { "style": "scss", "linter": "eslint", "babel": true },
      "component": { "style": "scss" },
      "library": { "style": "scss", "linter": "eslint" }
    }
  },
  "projects": {
    "engine": {
      "root": "libs/engine",
      "sourceRoot": "libs/engine/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/engine/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/engine"],
          "options": {
            "jestConfig": "libs/engine/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/engine",
            "tsConfig": "libs/engine/tsconfig.lib.json",
            "packageJson": "libs/engine/package.json",
            "main": "libs/engine/src/index.ts",
            "assets": ["libs/engine/*.md"]
          }
        }
      }
    },
    "console": {
      "root": "apps/console",
      "sourceRoot": "apps/console/src",
      "projectType": "application",
      "prefix": "console",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/console",
            "main": "apps/console/src/main.ts",
            "tsConfig": "apps/console/tsconfig.app.json",
            "assets": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/console/src/environments/environment.ts",
                  "with": "apps/console/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "console:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/console/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/console"],
          "options": {
            "jestConfig": "apps/console/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "playground": {
      "root": "apps/playground",
      "sourceRoot": "apps/playground/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/playground",
            "index": "apps/playground/src/index.html",
            "main": "apps/playground/src/main.tsx",
            "polyfills": "apps/playground/src/polyfills.ts",
            "tsConfig": "apps/playground/tsconfig.app.json",
            "assets": [
              "apps/playground/src/favicon.ico",
              "apps/playground/src/assets"
            ],
            "styles": ["apps/playground/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "apps/playground/webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/playground/src/environments/environment.ts",
                  "with": "apps/playground/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "playground:build",
            "proxyConfig": "apps/playground/proxy.conf.json"
          },
          "configurations": {
            "production": { "buildTarget": "playground:build:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/playground/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/playground"],
          "options": {
            "jestConfig": "apps/playground/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "playground-e2e": {
      "root": "apps/playground-e2e",
      "sourceRoot": "apps/playground-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/playground-e2e/cypress.json",
            "tsConfig": "apps/playground-e2e/tsconfig.e2e.json",
            "devServerTarget": "playground:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "playground:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/playground-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "server": {
      "root": "apps/server",
      "sourceRoot": "apps/server/src",
      "projectType": "application",
      "prefix": "server",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/server",
            "main": "apps/server/src/main.ts",
            "tsConfig": "apps/server/tsconfig.app.json",
            "assets": ["apps/server/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/server/src/environments/environment.ts",
                  "with": "apps/server/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "server:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/server/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/server"],
          "options": {
            "jestConfig": "apps/server/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "app": {
      "root": "apps/app",
      "sourceRoot": "apps/app/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/app",
            "index": "apps/app/src/index.html",
            "main": "apps/app/src/main.tsx",
            "polyfills": "apps/app/src/polyfills.ts",
            "tsConfig": "apps/app/tsconfig.app.json",
            "assets": ["apps/app/src/favicon.ico", "apps/app/src/assets"],
            "styles": ["apps/app/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/app/src/environments/environment.ts",
                  "with": "apps/app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": { "buildTarget": "app:build" },
          "configurations": {
            "production": { "buildTarget": "app:build:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/app/**/*.{ts,tsx,js,jsx}"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/app"],
          "options": {
            "jestConfig": "apps/app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "app-e2e": {
      "root": "apps/app-e2e",
      "sourceRoot": "apps/app-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/app-e2e/cypress.json",
            "tsConfig": "apps/app-e2e/tsconfig.e2e.json",
            "devServerTarget": "app:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "app:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/app-e2e/**/*.{js,ts}"] }
        }
      }
    },
    "engine-components": {
      "root": "libs/engine-components",
      "sourceRoot": "libs/engine-components/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/engine-components/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/engine-components"],
          "options": {
            "jestConfig": "libs/engine-components/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": { "configFolder": "libs/engine-components/.storybook" }
          },
          "configurations": { "ci": { "quiet": true } }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/engine-components",
            "config": { "configFolder": "libs/engine-components/.storybook" }
          },
          "configurations": { "ci": { "quiet": true } }
        }
      }
    },
    "engine-components-e2e": {
      "root": "apps/engine-components-e2e",
      "sourceRoot": "apps/engine-components-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/engine-components-e2e/cypress.json",
            "tsConfig": "apps/engine-components-e2e/tsconfig.e2e.json",
            "devServerTarget": "engine-components:storybook"
          },
          "configurations": {
            "ci": { "devServerTarget": "engine-components:storybook:ci" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/engine-components-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
