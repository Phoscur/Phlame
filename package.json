{
  "name": "phlame",
  "version": "0.0.1",
  "license": "AGPLv3",
  "scripts": {
    "nx": "nx",
    "start": "nx serve app",
    "dev": "nx run-many --target=serve --projects=server,app --parallel",
    "dev-e2e": "nx e2e app-e2e --watch",
    "cypress": "cd app-e2e && cypress open",
    "build": "nx build",
    "test": "nx run-many -t test --parallel --output-style stream",
    "ci": "nx run-many -t test -p app engine --parallel --verbose --coverage --nx-bail",
    "lint": "nx report && nx run-many -t lint",
    "e2e": "nx run-many -t e2e"
  },
  "private": true,
  "resolutions": {},
  "devDependencies": {
    "@nx/cypress": "18.0.5",
    "@nx/eslint": "18.0.5",
    "@nx/eslint-plugin": "18.0.5",
    "@nx/js": "18.0.5",
    "@nx/vite": "18.0.5",
    "@nx/web": "18.0.5",
    "@swc-node/register": "~1.8.0",
    "@swc/core": "~1.3.85",
    "@swc/helpers": "~0.5.2",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "cypress": "^13.6.6",
    "eslint": "~8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.13.4",
    "jsdom": "~22.1.0",
    "nx": "18.0.5",
